{
  "manifest_version": 2,
  "name": "Twitch Channel Points Auto Clicker",
  "short_name": "TCPAC",
  "version": "0.0.12",
  "description": "Auto clicks the Channel Points bonus chest for you, so you can watch streams in peace.",
  "content_scripts": [
    {
      "matches": ["*://*.twitch.tv/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
