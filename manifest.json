{
	"manifest_version": 2,
	"name": "Twitch Channel Points Auto Clicker",
	"short_name": "TCPAC",
	"version": "0.0.11",
	"description": "Enjoy streams in peace!",
	"content_scripts": [
		{
			"matches": ["*://*.twitch.tv/*"],
			"js": ["content.js"],
			"css": ["deveasteregg.css"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {},
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
